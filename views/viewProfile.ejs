<!doctype html>
<html>

<head>
  <meta charset="utf-8" />
<meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
  <title>Safe-Haven</title>
  <link rel="canonical" href="https://getbootstrap.com/docs/4.0/examples/sign-in/">
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
  <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css">
  <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css">
  <!-- <link rel="stylesheet" href="style.css"> -->
  <link rel="stylesheet" href="../assets/css/main.css" />
  <noscript>
    <link rel="stylesheet" href="../assets/css/noscript.css" /></noscript>
</head>

<body class="is-preload">
  <div class="container">

    <div class="page-header">
      <h1>Guest Profile Page</h1>
      <div class="row">
        <div class="col-md-6">
          <div class="imgX" style="background-image:
             url('https://safe-havre.s3.us-east-2.amazonaws.com/<%= user.profileImg %>')"></div>
        </div>
                  <div class="well col-md-6">
                      <h3><span class="fa fa-user"></span> Profile</h3>

                      <% if (user.local.email) { %>
                          <p>
                              <strong>First Name</strong>: <%= user.local.firstName %><br>
                              <strong>Last Name</strong>: <%= user.local.lastName %><br>
                              <strong>sex</strong>: <%= user.local.sex %><br>
                              <strong>Age</strong>: <%= user.local.age %><br>
                              <strong>userType</strong>: <%= user.local.userType %>
                          </p>

                  </div>
              </div>
      <h2><%= user.email %></h2>
    </div>


    <div id="wrapper">
      <!-- Header -->
      <%- include('header'); -%>

      <!-- Main -->



    </div>
      <% } %>
    <script src="main.js"></script>
    <!-- Scripts -->
			<script src="../assets/js/jquery.min.js"></script>
			<script src="../assets/js/browser.min.js"></script>
			<script src="../assets/js/breakpoints.min.js"></script>
			<script src="../assets/js/util.js"></script>
			<script src="../assets/js/main.js"></script>
</body>

</html>
